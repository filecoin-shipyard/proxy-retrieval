{
  "name": "proxy-retrieval",
  "version": "1.0.0",
  "description": "Allow a browser node to retrieve from a storage miner",
  "main": "src/index.ts",
  "scripts": {
    "start": "yarn migrate && ts-node src/index.ts",
    "dev": "yarn migrate && ts-node-dev --no-notify src/index.ts",
    "lotus": "ts-node-dev --no-notify src/services/lotus.ts",
    "migrate": "knex migrate:latest --cwd src/db",
    "migrate-make": "knex migrate:make --cwd src/db"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mgoelzer/proxy-retrieval.git"
  },
  "bugs": {
    "url": "https://github.com/mgoelzer/proxy-retrieval/issues"
  },
  "homepage": "https://github.com/mgoelzer/proxy-retrieval#readme",
  "dependencies": {
    "axios": "^0.20.0",
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.6",
    "pg": "^8.4.1",
    "socket.io": "^2.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "unique-filename": "^1.1.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.11.2",
    "@types/pg": "^7.14.5",
    "@types/socket.io": "^2.1.11",
    "chalk": "^4.1.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "ts-node-dev": "^1.0.0-pre.63"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,json,css,md}": "prettier --write"
  }
}
